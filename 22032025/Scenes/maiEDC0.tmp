[sub_resource type="GDScript" id="GDScript_a2iyr"]
script/source = "extends Timer

# Reference to the creature that this timer will monitor
var creature_node: Node2D

# This function is called when the timer is ready
func _ready():
	if not is_connected(\"timeout\", Callable(self, \"_on_timeout\")):
		connect(\"timeout\", Callable(self, \"_on_timeout\"))

func set_creature(creature: Area2D):
	# Store the creature reference for path recording or other purposes
	self.creature = creature

# This function is called whenever the timer times out
func _on_timeout():
	if creature_node:
		print(\"DEBUG - Creature Path Points:\", creature_node.path_points)
		print(\"DEBUG - Creature Current Path Index:\", creature_node.path_index)
		print(\"DEBUG - Creature Global Position:\", creature_node.global_position)
"

